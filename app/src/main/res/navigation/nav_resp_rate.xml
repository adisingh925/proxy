<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/measureRespFragment">
    <fragment
        android:id="@+id/measureRespFragment"
        android:name="app.android.heartrate.phoneapp.fragments.health.resp_rate.subs.measure.MeasureRespRateFragment"
        android:label="Measure Resp Rate"
        tools:layout="@layout/fragment_measure_resp_rate">
        <action
            android:id="@+id/action_measureRespFragment_to_analyticsRespFragment"
            app:destination="@id/analyticsRespFragment" />
    </fragment>

    <fragment
        android:id="@+id/analyticsRespFragment"
        android:name="app.android.heartrate.phoneapp.fragments.health.resp_rate.subs.analytics.AnalyticsRespRateFragment"
        android:label="Analytics Resp Rate"
        tools:layout="@layout/fragment_analytics_resp_rate" />


</navigation>
